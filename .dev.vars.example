# Prisma Postgres (https://console.prisma.io で取得)
DATABASE_URL="postgresql://user:password@aws-0-region.prisma-postgres.net:5432/database"

# 管理者メール許可リスト用（seed スクリプトで使用）
ADMIN_EMAIL="admin@example.com"

# Resend（回答コピーメール送信、https://resend.com で取得）
RESEND_API_KEY=""

NISSHI_DEV_SURVEY_API_KEY="nisshi-dev-survey-api-key"

# Google OAuth（https://console.cloud.google.com で取得）
GOOGLE_CLIENT_ID="your-google-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# better-auth（openssl rand -base64 32 で生成）
BETTER_AUTH_SECRET="your-random-secret-at-least-32-chars"
BETTER_AUTH_URL="http://localhost:8787"
ALLOWED_ORIGINS="http://localhost:5173"
